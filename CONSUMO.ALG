Algoritmo "consumo"

Var
   distancia, combustivel, consumoMedio: real


Inicio
   escreva("Distancia percorrida: ")
   leia(distancia)
   escreva("Combustivel gasto: ")
   leia(combustivel)
   
   consumoMedio <- distancia / combustivel
   
   escreva("Consumo medio: ",consumoMedio:8:3)



Fimalgoritmo