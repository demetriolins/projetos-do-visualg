Algoritmo "salario"

Var
   nome: caractere
   salario: real
   idade: inteiro

Inicio
   escreva("Digite um nome: ")
   leia(nome)
   escreva("Digite o valor do salário: ")
   leia(salario)
   escreva("Digite a idade: ")
   leia(idade)
   
   escreva("A funcionária ", nome, " recebe um salario de ", salario, " e tem ", idade, " anos de idade")


Fimalgoritmo